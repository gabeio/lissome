{
  "name": "lissome",
  "version": "0.0.9",
  "private": true,
  "bugs": "https://github.com/gabeio/lissome/issues",
  "engines": {
    "node": ">= 0.10.0",
    "iojs": ">= 1.0.0"
  },
  "scripts": {
    "start": "node ./lib/app.js",
    "build": "gulp build",
    "clean": "gulp clean",
    "test": "gulp clean && gulp build && gulp build-tests && mocha --slow 2",
    "testCi": "gulp clean && gulp build && gulp build-tests && istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -s 2 -R spec",
    "coverage": "gulp build && gulp build-tests && istanbul cover ./node_modules/mocha/bin/_mocha",
    "continuous": "nodemon -w ./ -e html,css,js -x node ./lib/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gabeio/lissome.git"
  },
  "dependencies": {
    "async": "~1.5.0",
    "bcrypt": "~0.8.5",
    "body-parser": "~1.14.2",
    "compression": "~1.6.0",
    "connect-redis": "~3.0.1",
    "cors": "~2.7.1",
    "csurf": "~1.8.3",
    "express": "~4.13.3",
    "express-partial-response": "~0.3.4",
    "express-session": "~1.13.0",
    "helmet": "~1.0.1",
    "ioredis": "~1.13.2",
    "kerberos": "~0.0.17",
    "livescript": "~1.4.0",
    "lodash": "~3.10.1",
    "markdown-it": "~5.0.2",
    "method-override": "~2.3.5",
    "moment": "~2.10.6",
    "moment-timezone": "~0.5.0",
    "mongoose": "~4.3.4",
    "multer": "~1.1.0",
    "passcode": "~1.0.2",
    "response-time": "~2.3.1",
    "request": "~2.67.0",
    "serve-static": "~1.10.0",
    "swig": "~1.4.2",
    "thirty-two": "~1.0.1",
    "winston": "~2.1.1",
    "yargs": "~3.31.0"
  },
  "optionalDependencies": {
    "hiredis": "~0.4.1"
  },
  "devDependencies": {
    "chai": "~3.4.1",
    "del": "~2.2.0",
    "gulp": "~3.9.0",
    "gulp-livescript": "~3.0.0",
    "istanbul": "~0.4.0",
    "mocha": "~2.3.3",
    "supertest": "~0.15.0"
  }
}
